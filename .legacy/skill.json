{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-GB": {
          "summary": "Learn fun facts about your county.",
          "description": "Ask for facts about your local county! Make sure to enable location and name permissions so I know which facts to share and who I'm talking to!",
          "smallIconUri": "https://raw.githubusercontent.com/Jack-Corbett/County-Facts/master/Small%20Icon.png",
          "largeIconUri": "https://raw.githubusercontent.com/Jack-Corbett/County-Facts/master/Large%20Icon.png",
          "examplePhrases": [
            "Alexa open county facts",
            "Alexa ask county facts for a fact",
            "Alexa tell county facts to tell me a fact"
          ],
          "keywords": [
            "county",
            "facts",
            "information",
            "learn"
          ],
          "name": "County Facts"
        }
      },
      "distributionCountries": [
        "GB"
      ],
      "isAvailableWorldwide": false,
      "distributionMode": "PUBLIC",
      "testingInstructions": "TODO",
      "category": "KNOWLEDGE_AND_TRIVIA"
    },
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "usesPersonalInfo": false,
      "isChildDirected": false,
      "isExportCompliant": true,
      "containsAds": false,
      "locales": {
        "en-GB": {
          "privacyPolicyUrl": "https://github.com/Jack-Corbett/County-Facts/blob/master/Privacy%20Policy.md",
          "termsOfUseUrl": ""
        }
      }
    },
      "category": "KNOWLEDGE_AND_TRIVIA"
    },
    "permissions": [
      {
        "name": "alexa::devices:all:address:full:read"
      },
      {
        "name": "alexa::profile:name:read"
      }
    ],
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom",
          "uri": "ask-custom-County-Facts-default"
        }
      }
    },
    "manifestVersion": "1.0"
  }
}
